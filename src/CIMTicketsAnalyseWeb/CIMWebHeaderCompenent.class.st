{ #category : #rendering }
CIMWebHeaderCompenent >> renderContentOn: html [
	html mdlLayout
		with: [ html
				mdlLayoutHeader: [ html
						mdlLayoutHeaderRow: [ html mdlLayoutTitle: 'TicketsDashboard'.
							html mdlLayoutSpacer.
							"html mdlIcon: (self icon: self rmodLogoUrl) "] ] ]
]

